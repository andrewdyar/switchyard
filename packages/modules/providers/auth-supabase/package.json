{
  "name": "@switchyard/auth-supabase",
  "version": "1.0.0",
  "description": "Supabase authentication provider for Medusa with RBAC support",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewdyar/switchyard",
    "directory": "packages/modules/providers/auth-supabase"
  },
  "files": [
    "dist",
    "!dist/**/__tests__",
    "!dist/**/__mocks__",
    "!dist/**/__fixtures__"
  ],
  "engines": {
    "node": ">=20"
  },
  "author": "Goods Grocery",
  "license": "MIT",
  "scripts": {
    "test": "../../../../node_modules/.bin/jest --passWithNoTests src",
    "test:integration": "../../../../node_modules/.bin/jest --passWithNoTests --forceExit --testPathPattern=\"integration-tests/__tests__/.*\\.spec\\.ts\"",
    "build": "yarn run -T rimraf dist && yarn run -T tsc --build",
    "watch": "yarn run -T tsc --watch"
  },
  "devDependencies": {
    "@switchyard/framework": "workspace:*"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0"
  },
  "peerDependencies": {
    "@switchyard/framework": "workspace:*"
  },
  "keywords": [
    "medusa-provider",
    "medusa-provider-auth-supabase"
  ]
}
